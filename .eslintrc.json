{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "extends": ["eslint-config-amuench", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": ["./apps/next/tsconfig.json", "./apps/electron/tsconfig.json"]
  },
  "rules": {
    "import-newlines/enforce": 0
  }
}
